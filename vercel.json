{
	"version": 2,
	"builds": [
		{
			"src": "backend/dist/server.js",
			"use": "@vercel/node"
		}
	],
	"routes": [
		{
			"src": "/api/(.*)",
			"dest": "backend/dist/server.js"
		},
		{
			"src": "/(.*)",
			"dest": "frontend/dist/$1"
		}
	],
	"build": {
		"env": {
			"NODE_ENV": "production"
		}
	}
}
